#include "person.h"
#include <string>

using namespace std;

Person::Person(string n, string a, string t, string e)
{  
   this->name = n;
   this->address = a;
   this->telephone = t;
   this->email = e;
}
string Person::get_name()
{
   return name;
}
string Person::get_address()
{  
   return address;
}
string Person::get_telephone()
{
   return telephone;
}

string Person::get_email()
{
   return email;
}


