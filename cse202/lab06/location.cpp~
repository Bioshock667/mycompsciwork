#include "location.h"
#include <string>

using namespace std;

Location::Location()
{
    string address = "";
    string building = "";
    string room = "";
}

Location::Location(string addr, atring bldg, string rm)
{
    address = addr;
    building = bldg;
    room = rm;
}

string Location::getLocationAddress()
{
    return address;
}

string Location::getLocationBuilding()
{
    return building;
}

string Location::getLocationRoom()
{
    return room;
}
